{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "./node_modules/.bin/tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@google-cloud/datastore": "^4.2.0",
    "@types/cheerio": "^0.22.13",
    "@types/client-sessions": "^0.8.0",
    "@types/date-and-time": "^0.6.0",
    "@types/luxon": "^1.15.2",
    "@types/node-fetch": "^2.5.0",
    "@types/oauth": "^0.9.1",
    "@types/passport-twitter": "^1.0.34",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-jss": "^8.6.4",
    "@types/twit": "^2.2.22",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "client-sessions": "^0.8.0",
    "compression": "^1.7.4",
    "date-and-time": "^0.9.0",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-validator": "^6.0.1",
    "greenlock-express": "^2.7.16",
    "greenlock-store-fs": "^3.0.2",
    "jss": "^9.8.7",
    "lodash": "^4.17.13",
    "lusca": "^1.6.1",
    "luxon": "^1.17.2",
    "node-fetch": "^2.6.7",
    "oauth": "^0.9.15",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-jss": "^8.6.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "twit": "^2.2.11",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-flash": "0.0.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/lusca": "^1.6.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.3.9",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
